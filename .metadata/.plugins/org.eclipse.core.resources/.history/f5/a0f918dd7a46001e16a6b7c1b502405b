package NumerRaisedToNumber;

public class NumberCounting 
{
void countAndPower(int n)
{
	int org=n;
	int fin=1;
	int count=0;
	while(n>0)
	{
		n=n/10;
		count++;
		
	}
	System.out.println(count);
	
	for(int i=1;i<=count;i++)
	{
		fin=fin*org;
	}
	System.out.println(fin);
	//return count;
}
}
